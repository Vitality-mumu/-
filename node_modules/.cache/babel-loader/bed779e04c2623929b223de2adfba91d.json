AAA,CAAA,YAAA,CAAA,KAAA,EAAA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,IAAA,CAAA,KAAA,OAAA,CAAA,YAAA,CAAA,KAAA,CAAA,CAAA,CAAA,EADA,CACA;AACA;;AACA,QAAA,MAAA,GAAA,IAAA,IAAA,EAAA;AACA,SAAA,OAAA,GAAA,IAAA,IAAA,WACA,MAAA,CAAA,WAAA,EADA,cACA,MAAA,CAAA,QAAA,KAAA,CADA,cACA,MAAA,CAAA,OAAA,EADA,EAAA;AAGA;AAtGA,CAAA","sourcesContent":["<template>\r\n  <div class=\"detail\">\r\n    <van-form @submit=\"onSubmitClick\">\r\n      <!-- 身份 -->\r\n      <van-field name=\"infor.radio\">\r\n        <template #input>\r\n          <van-radio-group\r\n            v-model=\"infor.radio\"\r\n            direction=\"horizontal\"\r\n            @change=\"onChangeClick\"\r\n          >\r\n            <van-radio name=\"1\" label-position=\"left\"\r\n              >肿瘤患者\r\n              <template #icon=\"props\">\r\n                <img\r\n                  class=\"img-icon\"\r\n                  :src=\"props.checked ? activeIcon : inactiveIcon\"\r\n                />\r\n              </template>\r\n            </van-radio>\r\n            <van-radio name=\"2\" label-position=\"left\"\r\n              >病患家属\r\n              <template #icon=\"props\">\r\n                <img\r\n                  class=\"img-icon\"\r\n                  :src=\"props.checked ? activeIcon : inactiveIcon\"\r\n                />\r\n              </template>\r\n            </van-radio>\r\n          </van-radio-group>\r\n        </template>\r\n      </van-field>\r\n\r\n      <!-- 姓名 -->\r\n      <div class=\"span\">患者姓名</div>\r\n      <span class=\"required\"> *</span>\r\n      <van-field\r\n        v-model=\"infor.name\"\r\n        name=\"pattern\"\r\n        placeholder=\"请输入患者姓名\"\r\n        :rules=\"[{ required: true }]\"\r\n      />\r\n\r\n      <!-- 生日 -->\r\n      \r\n      <div class=\"span\">患者生日</div>\r\n      <span class=\"required\"> *</span>\r\n      <div class=\"time\">\r\n      <van-field\r\n        readonly\r\n        clickable\r\n        name=\"name\"\r\n        :value=\"birthdayDate\"\r\n        placeholder=\"点击选择日期\"\r\n        @click=\"showCalendar1 = true\"\r\n      >\r\n      </van-field>\r\n      <van-calendar\r\n        v-model=\"showCalendar1\"\r\n        @confirm=\"onConfirm1\"\r\n        :max-date=\"maxDate\"\r\n        :min-date=\"minDate\"\r\n      />\r\n       <img src=\"../../assets/time.png\" alt=\"\" />\r\n      </div>\r\n\r\n      <!-- 确诊日期 -->\r\n     \r\n      <div class=\"span\">患者确诊日期</div>\r\n      <span class=\"required\"> *</span>\r\n       <div class=\"time\">\r\n      <van-field\r\n        readonly\r\n        clickable\r\n        name=\"confirmed\"\r\n        :value=\"confirmedDate\"\r\n        placeholder=\"点击选择日期\"\r\n        @click=\"showCalendar2 = true\"\r\n      />\r\n      <van-calendar\r\n        v-model=\"showCalendar2\"\r\n        @confirm=\"onConfirm2\"\r\n        :max-date=\"maxDate\"\r\n        :min-date=\"minDate\"\r\n      />\r\n       <img src=\"../../assets/time.png\" alt=\"\" />\r\n      </div>\r\n      <!-- 手术日期 -->\r\n      \r\n      <div class=\"span\">首次手术日期</div>\r\n      <div class=\"time\">\r\n      <van-field\r\n        readonly\r\n        clickable\r\n        name=\"operation\"\r\n        :value=\"operationDate\"\r\n        placeholder=\"点击选择日期\"\r\n        @click=\"showCalendar3 = true\"\r\n      />\r\n      <van-calendar\r\n        v-model=\"showCalendar3\"\r\n        @confirm=\"onConfirm3\"\r\n        :max-date=\"maxDate\"\r\n        :min-date=\"minDate\"\r\n      />\r\n       <img src=\"../../assets/time.png\" alt=\"\" />\r\n      </div>\r\n      <!-- 首次放化疗时期 -->\r\n     \r\n      <div class=\"span\">首次放化疗日期</div>\r\n       <div class=\"time\">\r\n      <van-field\r\n        readonly\r\n        clickable\r\n        name=\"firstChemoradiotherapy\"\r\n        :value=\"firstChemoradiotherapy\"\r\n        placeholder=\"点击选择日期\"\r\n        @click=\"showCalendar4 = true\"\r\n      />\r\n      <van-calendar\r\n        v-model=\"showCalendar4\"\r\n        @confirm=\"onConfirm4\"\r\n        :max-date=\"maxDate\"\r\n        :min-date=\"minDate\"\r\n      />\r\n       <img src=\"../../assets/time.png\" alt=\"\" />\r\n      </div>\r\n      <!-- 最近一次放化疗时期 -->\r\n      <div class=\"span\">最近一次放化疗时期</div>\r\n        <div class=\"time\">\r\n          <van-field\r\n            readonly\r\n            clickable\r\n            name=\"latestChemoradiotherapy\"\r\n            :value=\"latestChemoradiotherapy\"\r\n            placeholder=\"点击选择日期\"\r\n            @click=\"showCalendar5 = true\"\r\n          />\r\n          <van-calendar\r\n            v-model=\"showCalendar5\"\r\n            @confirm=\"onConfirm5\"\r\n            :max-date=\"maxDate\"\r\n            :min-date=\"minDate\"\r\n          />\r\n          <img src=\"../../assets/time.png\" alt=\"\" />\r\n        </div>\r\n      <!-- 提交 -->\r\n      <div style=\"margin: 16px;\" class=\"font\">\r\n        <van-button\r\n          round\r\n          block\r\n          type=\"info\"\r\n          native-type=\"submit\"\r\n          :class=\"\r\n            infor.name != '' &&\r\n            infor.birthdayDate != '' &&\r\n            infor.confirmedDate != ''\r\n              ? ''\r\n              : 'disabled'\r\n          \"\r\n          :disabled=\"\r\n            infor.name != '' &&\r\n            infor.birthdayDate != '' &&\r\n            infor.confirmedDate != ''\r\n              ? false\r\n              : true\r\n          \"\r\n          >下一步</van-button\r\n        >\r\n      </div>\r\n\r\n      <div class=\"bottom\">\r\n        <span>平安肿瘤康复会对您的个人信息严格保密,所有</span>\r\n        <span>信息仅供对您的康复项目使用</span>\r\n      </div>\r\n    </van-form>\r\n    <my-child ref=\"child\"></my-child>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from \"../../http\";\r\nimport { Toast } from \"vant\";\r\nimport MyChild from \"../../components/MyChild.vue\";\r\nexport default {\r\n  components: { MyChild },\r\n  data() {\r\n    return {\r\n      activeIcon: require(\"../../../src/assets/active.png\"),\r\n      inactiveIcon: require(\"../../../src/assets/icon.png\"),\r\n      showCalendar1: false,\r\n      showCalendar2: false,\r\n      showCalendar3: false,\r\n      showCalendar4: false,\r\n      showCalendar5: false,\r\n      birthdayDate: \"\",\r\n      confirmedDate: \"\",\r\n      operationDate: \"\",\r\n      firstChemoradiotherapy: \"\",\r\n      latestChemoradiotherapy: \"\",\r\n      maxDate: \"\",\r\n      minDate: new Date(2000, 0, 1),\r\n\r\n      infor: {\r\n        //病症\r\n        diseaselist: [],\r\n        //身份\r\n        radio: \"1\",\r\n        //名字\r\n        name: \"\",\r\n        // 生日\r\n        birthdayDate: \"\",\r\n        //确诊日期\r\n        confirmedDate: \"\",\r\n        // 首次手术的时期\r\n        operationDate: \"\",\r\n        // 首次放化疗的日期\r\n        firstChemoradiotherapy: \"\",\r\n        // 最近一次放化疗时期\r\n        latestChemoradiotherapy: \"\",\r\n      },\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    //提交\r\n    onSubmitClick() {\r\n      this.sendinfor(this.infor);\r\n    },\r\n    //单选框\r\n    onChangeClick() {\r\n      console.log(this.infor.radio);\r\n    },\r\n    onConfirm1(date) {\r\n      this.birthdayDate = `${date.getFullYear()}年${date.getMonth() +\r\n        1}月${date.getDate()}日`;\r\n      this.infor.birthdayDate = this.formatTime(date);\r\n      this.showCalendar1 = false;\r\n    },\r\n    onConfirm2(date) {\r\n      this.confirmedDate = `${date.getFullYear()}年${date.getMonth() +\r\n        1}月${date.getDate()}日`;\r\n      this.infor.confirmedDate = this.formatTime(date);\r\n      this.showCalendar2 = false;\r\n    },\r\n    onConfirm3(date) {\r\n      this.operationDate = `${date.getFullYear()}年${date.getMonth() +\r\n        1}月${date.getDate()}日`;\r\n      this.infor.operationDate = this.formatTime(date);\r\n      this.showCalendar3 = false;\r\n    },\r\n    onConfirm4(date) {\r\n      this.firstChemoradiotherapy = `${date.getFullYear()}年${date.getMonth() +\r\n        1}月${date.getDate()}日`;\r\n      this.infor.firstChemoradiotherapy = this.formatTime(date);\r\n      this.showCalendar4 = false;\r\n    },\r\n    onConfirm5(date) {\r\n      this.latestChemoradiotherapy = `${date.getFullYear()}年${date.getMonth() +\r\n        1}月${date.getDate()}日`;\r\n      this.infor.latestChemoradiotherapy = this.formatTime(date);\r\n      this.showCalendar5 = false;\r\n    },\r\n    formatTime(Gdate) {\r\n      var moment = require(\"moment\");\r\n      let Ndate = moment(Gdate).format(\"YYYY-MM-DD\");\r\n      return Ndate;\r\n    },\r\n    async sendinfor(infor) {\r\n      try {\r\n        const res = await http.sendInfor(infor);\r\n        console.log(res);\r\n        this.$refs.child.show();\r\n      } catch {\r\n        Toast.fail(\"服务器繁忙，请稍后再试...\");\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    for (let i in this.$router.currentRoute.query) {\r\n      this.infor.diseaselist.push(this.$router.currentRoute.query[i]); //属性\r\n    }\r\n    var myDate = new Date();\r\n    this.maxDate = new Date(\r\n      `${myDate.getFullYear()},${myDate.getMonth() + 1},${myDate.getDate()}`\r\n    );\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.img-icon {\r\n  height: 24px;\r\n  width: 24px;\r\n}\r\n@backgroundcolor: #1989fa;\r\n\r\n.detail {\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: #fafafa;\r\n  height: 100%;\r\n}\r\n.span {\r\n  margin-top: 28px;\r\n  margin-left: 28px;\r\n  display: inline-block;\r\n  font-size: 30px;\r\n  font-family: Source Han Sans CN;\r\n  font-weight: 400;\r\n  line-height: 54px;\r\n  color: #333333;\r\n}\r\n.van-cell,\r\n.rank {\r\n  width: 694px;\r\n  height: 92px;\r\n  margin: 0px 28px;\r\n  background: #ffffff;\r\n  box-shadow: 0px 0px 12px rgba(199, 199, 199, 0.1);\r\n  border-radius: 10px;\r\n  margin-top: 20px;\r\n}\r\n/deep/.van-button--danger {\r\n    color: #fff;\r\n    background-color: #1989fa;\r\n    border: 1px solid #1989fa;\r\n}\r\n/deep/.van-calendar__selected-day {\r\n    width: 1.44rem;\r\n    height: 1.44rem;\r\n    color: #fff;\r\n    background-color: #1989fa;\r\n    border-radius: 0.10667rem;\r\n}\r\n.van-radio {\r\n  font-size: 28px;\r\n}\r\n// /deep/.van-button {\r\n//   width: 630px;\r\n//   height: 110px;\r\n//   background: #386ce2;\r\n//   margin-top: 74px 60px 140px 60px;\r\n// }\r\n\r\n.font {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.bottom {\r\n  font-family: Source Han Sans CN;\r\n  font-weight: 400;\r\n  color: #999999;\r\n  display: flex;\r\n  margin-top: 42px;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  span {\r\n    margin: 5px;\r\n    font-size: 20px;\r\n  }\r\n}\r\n.required {\r\n  color: red;\r\n}\r\n.disabled {\r\n  background: #3f3e3e;\r\n  border: 1px solid #3f3e3e;\r\n}\r\n.time {\r\n  position: relative;\r\n  img {\r\n    position: absolute;\r\n    right: 50px;\r\n    top: 30px;\r\n    width: 40px;\r\n    height: 38px;\r\n  }\r\n}\r\n\r\n.van-radio {\r\n    margin: auto;\r\n    margin-left: 70px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/pages/detail"}]}